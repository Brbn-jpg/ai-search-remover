{
  "manifest_version": 3,
  "name": "AI remover",
  "version": "1.0",
  "description": "Removes AI description from your searches",
  "permissions": ["storage", "webNavigation", "tabs"],
  "host_permissions": [
    "*://*.google.com/*",
    "*://*.google.pl/*",
    "*://*.bing.com/*",
    "*://*.duckduckgo.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/AiRemover16x16.png",
    "48": "icons/AiRemover48x48.png",
    "128": "icons/AiRemover128x128.png"
  }
}
